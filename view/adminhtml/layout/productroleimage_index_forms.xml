<?xml version="1.0"?>
<!--
    Layout for Product Role Image Add Form
    Renders the ethnic_productroleimage_form UI component inside the admin content container
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">

    <!-- Inherit admin styles -->
    <update handle="styles"/>

    <body>
        <referenceContainer name="content">
            <uiComponent name="ethnic_productroleimage_form"/>
        </referenceContainer>
    </body>
</page>
