<?xml version="1.0"?>
<!--
    Layout for Product Role Image Listing Page
    Displays the UI component grid: ethnic_productroleimage_listing
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">

    <!-- Load Magento backend styles -->
    <update handle="styles"/>

    <body>
        <referenceContainer name="content">
            <uiComponent name="ethnic_productroleimage_listing"/>
        </referenceContainer>
    </body>
</page>
